<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="https://github.com/avaloniaui"
        xmlns:vm="clr-namespace:LinuxFanControl.Gui.ViewModels;assembly=LinuxFanControl.Gui"
        xmlns:views="clr-namespace:LinuxFanControl.Gui.Views;assembly=LinuxFanControl.Gui"
        x:Class="LinuxFanControl.Gui.Views.MainWindow"
        Title="LinuxFanControl"
        Width="960" Height="640"
        Icon="/Assets/logo.ico"
        DataContext="{Binding Main, Source={StaticResource Locator}}">
  <Design.DataContext>
    <vm:MainWindowViewModel />
  </Design.DataContext>

  <Grid>
    <views:DashboardView />
  </Grid>
</Window>
